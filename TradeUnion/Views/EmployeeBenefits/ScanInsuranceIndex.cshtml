
@{
    ViewBag.Title = "ScanInsuranceIndex";
    Layout = "~/Views/Shared/_UnionLeft.cshtml";
}

<div class="col-md-11 well admin-content">
    <fieldset>
        <legend class="text-center">浏览教职工保险信息</legend>
        <div class="container">
            <div class="row col-md-6 custyle">
                <table class="table table-striped custab">
                    <thead>
                        <tr>
                            <th>姓名</th>
                            <th>保险</th>
                            <th>有效期</th>
                            <th>份数</th>
                            <th class="text-center">操作</th>
                        </tr>
                    </thead>
                    @{
                        foreach (var item in ViewBag.List)
                        {
                            <tr>
                                <td>@item.XingMing</td>
                                <td>@item.BaoXian</td>
                                <td>@item.YouXiaoQi</td>
                                <td>@item.FenSHu</td>
                                <td class="text-center">
                                    <a class='btn btn-info btn-xs' href="/EmployeeBenefits/EditInsuranceIndex?Id=@item.ID"><span class="glyphicon glyphicon-edit"></span> 编辑</a>
                                    <a href="/EmployeeBenefits/DelBaoXian?Id=@item.ID" class="btn btn-danger btn-xs"><span class="glyphicon glyphicon-remove"></span> 删除</a>
                                </td>
                            </tr>
                        }
                    }
                </table>
            </div>
        </div>
    </fieldset>
</div>

